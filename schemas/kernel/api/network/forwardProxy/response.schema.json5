{
    $schema: 'https://json-schema.org/draft/2020-12/schema',
    $id: 'https://github.com/siyuan-community/siyuan-sdk/raw/main/schemas/kernel/api/network/forwardProxy/response.schema.json5',
    $comment: 'v2.10.3',
    $ref: '#/$defs/root',
    $defs: {
