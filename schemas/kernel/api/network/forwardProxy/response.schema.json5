                },
            },
        },
        data: {
            title: 'IData',
            description: 'Response information',

            type: 'object',
            additionalProperties: false,
            required: ['url', 'status', 'elapsed', 'contentType', 'headers', 'body', 'bodyEncoding'],
            properties: {
                url: {
