                    type: 'integer',
                    description: 'response elapsed',
                    minimum: 0,
                },
                contentType: {
                    // 响应体内容类型
                    type: 'string',
                    description: 'response content-type',
                },
                headers: {
                    // 响应头
                    description: 'response headers',
